{% block stylesheets %}

    <!-- Vendor styles -->
    <link href="{{ asset('bundles/administracion/css/font-awesome.css') }}" rel="stylesheet">
    <link href="{{ asset('bundles/administracion/css/metisMenu.css') }}" rel="stylesheet">
    <link href="{{ asset('bundles/administracion/css/animate.css') }}" rel="stylesheet">
    <link href="{{ asset('bundles/administracion/css/bootstrap.css') }}" rel="stylesheet">
    <link href="{{ asset('bundles/administracion/css/dataTables.bootstrap.css') }}" rel="stylesheet">
    <link rel="stylesheet" href="{{ asset('bundles/administracion/css/alertify.core.css') }}" />
    <link rel="stylesheet" href="{{ asset('bundles/administracion/css/alertify.default.css') }}" />

    <link rel="stylesheet" href="{{ asset('bundles/administracion/css/awesome-bootstrap-checkbox.css') }}" />

    <!-- Royal Preloader CSS -->
    <link rel="stylesheet" type="text/css" href="{{ asset('bundles/administracion/css/royal_preloader.css') }}">


    <!-- App styles -->
    <link href="{{ asset('bundles/administracion/css/pe-icon-7-stroke.css') }}" rel="stylesheet">
    <link href="{{ asset('bundles/administracion/css/helper.css') }}" rel="stylesheet">
    <link href="{{ asset('bundles/administracion/css/style.css') }}" rel="stylesheet">
